table
  tr
    td
      width: auto !important
      float: none !important
      vertical-align: top

.dropdown
  position:absolute
  display:none
  z-index: 10
  color: #fff
  a
    padding: 8px !important

.breadcrumb
  margin: 8px 0px
  color: #fff
  font-size: 12px
  line-height: 1.5em
  overflow: hidden
  a
    text-decoration: none
    color: #fff
    &:hover
      color: #bbb
  i
    padding: 0 10px

